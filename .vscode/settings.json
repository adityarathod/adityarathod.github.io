{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.formatOnSave": true,
	"editor.tabSize": 2,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.options": {
		"extensions": [".js", ".jsx", ".md", ".mdx", ".ts", ".tsx"]
	},
	"eslint.validate": [
		"markdown",
		"mdx",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"rpc.enabled": false
}
